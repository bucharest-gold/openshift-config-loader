{
  "name": "openshift-config-loader",
  "version": "0.4.0",
  "description": "Loading of Openshift configurations",
  "main": "index.js",
  "scripts": {
    "lint": "xo",
    "test": "tape test/*-test.js | tap-spec",
    "coverage": "nyc npm test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "ci": "npm run lint && npm run coveralls",
    "release": "standard-version"
  },
  "xo": {
    "space": 2,
    "rules": {
      "prefer-object-spread": [
        "off"
      ],
      "space-before-function-paren": [
        "error",
        "always"
      ]
    }
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "index.js",
    "lib"
  ],
  "keywords": [
    "Openshift",
    "Node.js"
  ],
  "author": {
    "name": "Red Hat, Inc."
  },
  "maintainers": {
    "name": "Lucas Holmquist",
    "email": "lholmqui@redhat.com"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodeshift/openshift-config-loader"
  },
  "bugs": "https://github.com/nodeshift/openshift-config-loader/issues",
  "license": "Apache-2.0",
  "dependencies": {
    "js-yaml": "~3.12.1"
  },
  "devDependencies": {
    "coveralls": "~3.0.2",
    "nyc": "^12.0.2",
    "proxyquire": "~2.1.0",
    "standard-version": "~4.4.0",
    "tap-spec": "^5.0.0",
    "tape": "~4.9.2",
    "xo": "~0.23.0"
  }
}
